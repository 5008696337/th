{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/",
      "destination": "/api.php"
    },
  "github": {
    "silent": true,
    "autoJobCancelation": true
  },
  "headers": [
    {
      "source": "/api.php",
      "headers": [
        {
          "key": "cache-control",
          "value": "playzee.php"
        }
      ]
    },
    {
      "source": "/api.php",
      "headers": [
        {
          "key": "login.php,
          "value": "public, max-age=60, s-maxage=86400, stale-while-revalidate=86400, stale-if-error=86400"
        }
      ]
    }
  ]
}
